{
  "name": "@agent-hub/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Blockchain Agent Hub",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./abis": {
      "types": "./dist/abis/index.d.ts",
      "import": "./dist/abis/index.mjs",
      "require": "./dist/abis/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "viem": "^2.21.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "viem": ">=2.0.0"
  },
  "keywords": [
    "blockchain",
    "ai-agents",
    "hashkey-chain",
    "ethereum",
    "sdk"
  ],
  "license": "MIT"
}
