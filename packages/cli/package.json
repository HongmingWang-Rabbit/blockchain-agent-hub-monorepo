{
  "name": "@agent-hub/cli",
  "version": "0.1.0",
  "description": "CLI tool for Blockchain Agent Hub",
  "bin": {
    "agent-hub": "./dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs --clean",
    "dev": "tsup src/index.ts --format cjs --dts --watch",
    "start": "node dist/index.js",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@agent-hub/sdk": "*",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.0",
    "ora": "^8.0.1",
    "viem": "^2.21.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "cli",
    "blockchain",
    "ai-agents",
    "hashkey-chain"
  ],
  "license": "MIT"
}
